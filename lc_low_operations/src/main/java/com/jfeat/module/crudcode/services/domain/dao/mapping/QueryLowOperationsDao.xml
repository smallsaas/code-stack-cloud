        , lc_low_operations.title, lc_low_operations.type, lc_low_operations.path, lc_low_operations.outside, lc_low_operations.request_api AS requestApi, lc_low_operations.request_refresh_api AS requestRefreshApi, lc_low_operations.request_method AS requestMethod, lc_low_operations.request_body AS requestBody, lc_low_operations.request_options AS requestOptions, lc_low_operations.page_id AS pageId, lc_low_operations.expect_field AS expectField, lc_low_operations.expect_value AS expectValue, lc_low_operations.modal_title AS modalTitle, lc_low_operations.modal_width AS modalWidth, lc_low_operations.modal_layout AS modalLayout
    </sql>


    <select id="queryMasterModel" resultType="LowOperationsModel">
        SELECT lc_low_operations.*
        FROM lc_low_operations
        WHERE lc_low_operations.id = #{id}
        GROUP BY lc_low_operations.id
    </select>


