    <select id="findLowOperationsPage" resultType="LowOperationsRecord" parameterType="LowOperationsRecord">
        SELECT
        <include refid="Base_Column_List"/>


        FROM lc_low_operations


        WHERE 1=1


        <if test="record.id != null and record.id>0 ">
            AND lc_low_operations.id LIKE CONCAT('%',#{record.id},'%')
        </if>
        <if test="record.title != null and record.title!= ''">
            AND lc_low_operations.title LIKE CONCAT('%',#{record.title},'%')
        </if>
        <if test="record.type != null and record.type!= ''">
            AND lc_low_operations.type LIKE CONCAT('%',#{record.type},'%')
        </if>
        <if test="record.path != null and record.path!= ''">
            AND lc_low_operations.path LIKE CONCAT('%',#{record.path},'%')
        </if>
        <if test="record.outside != null and record.outside>0 ">
            AND lc_low_operations.outside LIKE CONCAT('%',#{record.outside},'%')
        </if>


        <if test="record.requestApi != null and record.requestApi!= ''">
            AND lc_low_operations.request_api LIKE CONCAT('%',#{record.requestApi},'%')
        </if>


        <if test="record.requestRefreshApi != null and record.requestRefreshApi!= ''">
            AND lc_low_operations.request_refresh_api LIKE CONCAT('%',#{record.requestRefreshApi},'%')
        </if>


        <if test="record.requestMethod != null and record.requestMethod!= ''">
            AND lc_low_operations.request_method LIKE CONCAT('%',#{record.requestMethod},'%')
        </if>


        <if test="record.requestBody != null and record.requestBody!= ''">
            AND lc_low_operations.request_body LIKE CONCAT('%',#{record.requestBody},'%')
        </if>


        <if test="record.requestOptions != null and record.requestOptions!= ''">
            AND lc_low_operations.request_options LIKE CONCAT('%',#{record.requestOptions},'%')
        </if>


        <if test="record.pageId != null and record.pageId>0 ">
            AND lc_low_operations.page_id = #{record.pageId}
        </if>


        <if test="record.expectField != null and record.expectField!= ''">
            AND lc_low_operations.expect_field LIKE CONCAT('%',#{record.expectField},'%')
        </if>


        <if test="record.expectValue != null and record.expectValue!= ''">
            AND lc_low_operations.expect_value LIKE CONCAT('%',#{record.expectValue},'%')
        </if>


        <if test="record.modalTitle != null and record.modalTitle!= ''">
            AND lc_low_operations.modal_title LIKE CONCAT('%',#{record.modalTitle},'%')
        </if>


        <if test="record.modalWidth != null and record.modalWidth>0 ">
            AND lc_low_operations.modal_width LIKE CONCAT('%',#{record.modalWidth},'%')
        </if>


        <if test="record.modalLayout != null and record.modalLayout!= ''">
            AND lc_low_operations.modal_layout LIKE CONCAT('%',#{record.modalLayout},'%')
        </if>

        <if test="startTime != null">
            <![CDATA[AND lc_low_operations.start_time >= DATE(#{startTime})]]>
        </if>
        <if test="endTime != null">
            <![CDATA[AND lc_low_operations.end_time <= DATE(#{endTime}]]>
        </if>
        <!--
    <if test="search != null and search != ''">
        OR lc_low_operations.name LIKE CONCAT('%',#{search},'%')
    </if>
    -->
    </select>
